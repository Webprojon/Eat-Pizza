generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Products {
  id              String   @id @default(cuid())
  itemCategory    String
  itemImg         String
  itemName        String
  itemDescription String?
  itemPrice       Int
  updateAt        DateTime @updatedAt
  createAt        DateTime @default(now())
}
